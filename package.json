{
  "name": "svt-aws-db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "plan": "cd terraform && terraform init && terraform plan",
    "build:env": "node ./env.js",
    "deploy:db": "cd terraform && terraform init && terraform apply -auto-approve && cd ../ && npm run build:env",
    "dev": "sst dev",
    "build:sst": "sst build",
    "deploy": "npm run deploy:db && sst deploy",
    "remove": "sst remove && cd terraform && terraform destroy -auto-approve",
    "console": "sst console",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.1",
    "aws-cdk-lib": "2.91.0",
    "constructs": "10.2.69",
    "sst": "2.24.22",
    "typescript": "^5.2.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "@t3-oss/env-core": "^0.6.1",
    "dotenv": "^16.3.1",
    "influx": "^5.9.3",
    "zod": "^3.22.2"
  }
}
